{
  "expo": {
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/adaptive-icon.png"
      },
      "package": "com.kro.aKro",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "eeb912f0-fd9e-4842-af4f-8a2dd703d700"
      },
      "router": {}
    },
    "icon": "./assets/images/logo/icon.png",
    "ios": {
      "bundleIdentifier": "com.kro.aKro",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "supportsTablet": true
    },
    "name": "aKro",
    "newArchEnabled": true,
    "orientation": "portrait",
    "owner": "kro",
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/logo/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/outfit"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "android": {
            "permissions": [
              "android.permission.CAMERA",
              "android.permission.READ_EXTERNAL_STORAGE"
            ]
          },
          "cameraPermission": "akro souhaite accéder à votre appareil photo",
          "ios": {
            "NSPhotoLibraryUsageDescription": "akro souhaite accéder à vos photos pour créer votre profil"
          },
          "photosPermission": "akro souhaite accéder à vos photos"
        }
      ],
      "expo-audio"
    ],
    "scheme": "akro",
    "slug": "aKro",
    "userInterfaceStyle": "automatic",
    "version": "1.0.0",
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png",
      "output": "static"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/eeb912f0-fd9e-4842-af4f-8a2dd703d700"
    }
  }
}
